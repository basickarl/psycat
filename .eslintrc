{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "curly": [
            2,
            "all"
        ],
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}